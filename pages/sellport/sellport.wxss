page{
  background-color: #ebeff2;
}
.item {
  display: flex;
  padding: 20rpx;
  border-bottom: 1px solid #F4F4F4;
  background-color:#fff;
}
.spimg{
   width: 180rpx; 
   height:180rpx;
   background-size:cover;
   background-position: center;
   border-radius: 20rpx;
   border:2rpx solid #f5f5f5;
}
.item image{
  width: 180rpx;
  height: 180rpx;
  border-radius:20rpx;
  border:2rpx solid #f5f5f5;
}
.item .content {
  display: flex;
  flex-direction: column;
  padding:10rpx 0 0 30rpx;
}
.item .content .yscm{
  margin-top: 10rpx;
}
.sellprice{
  margin-top: 10rpx;
  display: flex;
  align-items: center;
  position: relative;
  width: 500rpx;
}
.sellprice text{
  color: #f04d52;
}
.sellprice view{
  position: absolute;
  right: 0;
  width: 100rpx;
  height: 100rpx;
  align-items: center;
  line-height: 100rpx;
}
.sellprice i{
  color: #0199FC;
}
.qty{
  margin-top: 10rpx;
}
.empty{
  display: flex;
  flex-direction: column;
  padding:100rpx;
}
.empty image{
  width:400rpx;
  height:200rpx;
  margin:auto;
}
.empty text{
  margin: auto;
  color: #C1C1C1;
  padding: 20rpx;
}
.child{
  display: flex;
  flex-direction: column;
  padding: 20rpx;
  color: #666;
}
.gotop{
  position: fixed;
  bottom: 100rpx;
  right: 50rpx;
  width: 140rpx;
  height: 140rpx;
}