page{
  background-color: #ebeff2;
}
.choose-goods{
  display:flex;
  flex-direction:column;
}
.dh{
  display: flex;
  align-items:center;
  padding: 26rpx;
  position: relative;
  background-color: #fff;
}
.dh .searchcion{
  position: absolute;
  left: 76rpx;
}
.dh input{
  background-color:#F0F4F5;
  padding:14rpx 76rpx;
  border-radius:50rpx;
  margin:0 20rpx;
  flex: 1;
}
.content{
  background-color: #fff;
  padding: 20rpx;
  display: flex;
  flex-direction: column;
  position: relative;
}
.content .orderinfo{
  font-size:32rpx;
  padding:10rpx;
  align-items: center;
  border-bottom: 2rpx solid #f5f5f5;
  color: #999;
}
.content .username{
  font-size:32rpx;
  padding:10rpx;
  display: flex;
  align-items: center;
  margin-top: 10rpx;
}
.content .username view{
  font-weight: bold;
}
.content .username text{
  margin-left: 16rpx;
}
.content .paystatus{
  position: absolute;
  top: 110rpx;
  right: 30rpx;
  color: #999;
}
.content .ordercode{
  font-size:32rpx;
  padding:10rpx;
}
.content .ordercode text:nth-child(1){
  font-weight: bold;
}
.content .ordercode text:nth-child(2){
  margin-left:16rpx;
}
.content .createtime{
  font-size:32rpx;
  padding:10rpx;
}
.content .createtime text:nth-child(1){
  font-weight: bold;
}
.content .createtime text:nth-child(2){
  margin-left:16rpx;
}
.content .amount{
  position: absolute;
  top: 180rpx;
  right: 30rpx;
  color: #f04d52;
}
.content .total{
  font-size:32rpx;
  padding:10rpx;
  position: absolute;
  top: 210rpx;
  right: 30rpx;
  color: #999;
}
.detail{
  padding: 0 30rpx;
  font-size: 32rpx;
  border-top: 1px solid #F4F4F4; 
  background-color:#fff;
}
.detail .detail-title {
  display: flex;
  padding:20rpx 0;
  font-weight: bold;
}
.detail .detail-title .spmc{
  width: 20%;
}
.detail .detail-title .sellprice{
  width: 20%;
}
.detail .detail-title view{
  width: 60%;
  display: flex;
}
.detail .detail-title view .sl{
  width: 30%;
  text-align:center;
}
.detail .detail-title view .returnsl{
  width: 30%;
  text-align:center;
}
.detail .detail-title view .okreturn{
  width: 40%;
  text-align:center;
}
.detail .child{
  display: flex;
  flex-direction: column;
  border-top: 1px solid #F4F4F4; 
}
.detail .child .spmc{
  display: flex;
  flex-direction: column;
  padding:10rpx  0;
}
.detail .child .spmc .mc{
  color: #333;
  font-weight: bold;
}
.detail .child .spmc view{
  padding-top: 20rpx;
}
.detail .child .spmc view text:nth-child(2){
  margin-left: 60rpx;
}
.detail .child .all {
  display: flex;
  align-items:center;
  padding:10rpx  0;
  border-top:1px dashed #F4F4F4;
}
.detail .child .all .spmc{
  width:20%;
}
.detail .child .all .sellprice{
   width:20%;
}
.detail .child .child-sl{
  width:60%;
  justify-content: center;
  align-items: center;
  display: flex;
}
.detail .child .sl{
  width: 30%;
  text-align:center;
} 
.detail .child .returnsl{
  width: 30%;
  text-align:center;
}
.detail .child .okreturn{
  width: 40%;
  text-align:center;
  display:flex;
  justify-content:center;
  align-items:center;
}
.detail .child .okreturn view{
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
}
.detail .child .okreturn i{
  font-size: 38rpx;
  color: red;
}
.detail .child .okreturn i:active{
  background-color:rgba(0, 0, 0, 0.1);
  opacity:0.7;
}
.detail .child .okreturn text{
  width: 80rpx;
  text-align: center;
}
.detail .child .okreturn image{
  width: 20rpx;
  height: 20rpx;
}
.wantthsl{
  text-align: right;
  color: #ED9823;
  font-size: 28rpx;
  padding: 10rpx 0;
}
.empty{
  display: flex;
  flex-direction: column;
  padding:100rpx;
}
.empty image{
  width:400rpx;
  height:200rpx;
  margin:auto;
}
.empty text{
  margin: auto;
  color: #C1C1C1;
  padding: 20rpx;
}
.return{
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  background-color: #fff;
  display: flex;
  z-index:2;
}
.return-btn{
  width: 30%;
  height: 100rpx;
  background-color: #f04d52;
  color: #fff;
  text-align: center;
  line-height: 100rpx;
  position:absolute;
  right:0;
}
.returnhistory{
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 60rpx;
}
.return-btn-null{
  width: 30%;
  height: 100rpx;
  background-color: #ccc;
  color: #fff;
  text-align: center;
  line-height: 100rpx;
  position:absolute;
  right:0;
}

