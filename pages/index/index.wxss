.search {
	width: 100%;
	height: 100rpx;
	position: absolute;
	top: 0;
	background-color: #303133;
}

.group {
	display: flex;
	align-items: center;
}

.group navigator {
	flex: 1;
}

.group navigator view {
	position: relative;
	display: flex;
	align-items: center;
}

.searchcion {
	position: absolute;
	left: 56rpx;
}

.search input {
	background-color: #F0F4F5;
	padding: 14rpx 76rpx;
	border-radius: 50rpx;
	margin: 0 20rpx;
	flex: 1;
}

.qrcode {
	display: flex;
	flex-direction: column;
	justify-content: center;
	margin-right: 20rpx;
	color: #fff;
}

.qrcode i {
	font-size: 60rpx;
}

.qrcode text {
	font-size: 18rpx;
	text-align: center;
}
.content {
	margin-top: 100rpx;
	margin-bottom: 130rpx;
}

.nav {
	width: 100%;
	display: flex;
}

.item {
	width: 250rpx;
	height: 250rpx;
}
.item i{
  color: #ED9823;
  font-size: 100rpx;
}
.item .th{
  font-size: 80rpx;
}
.item .dh{
  font-size: 80rpx;
}
.item .pd{
  font-size: 80rpx;
}
.item .cx{
  font-size: 80rpx;
}
.item .ck{
  font-size: 80rpx;
}
.item .sh{
  font-size: 80rpx;
}
.nav .item:nth-child(1),
.nav .item:nth-child(2) {
	border-right: 1px solid #F4F4F4;
}

.nav .item:nth-child(1),
.nav .item:nth-child(2),
.nav .item:nth-child(3) {
	border-bottom: 1px solid #F4F4F4;
}

.item navigator {
	width: 100%;
	height: 100%;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
}

.item image {
	width: 80rpx;
	height: 80rpx;
}

.item text {
	padding: 16rpx 0;
	color: #999;
}