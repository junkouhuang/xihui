.container{
  width:100%;
  height:388rpx;
  position: relative;
}
.card_bg{
  width:760rpx;
  height:388rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -194rpx;
  margin-left: -380rpx;
}
.cash{
  width: 100%;
  height: 388rpx;
  position:absolute;
  display: flex;
  align-items:center;
}
.freeze{
  display: flex;
  flex-direction: column;
  position: absolute;
  right: 30rpx;
}
.rental{
  display:flex;
  position:absolute;
  top:140rpx;
}
.rental text{
 color: #fff;
}
.freeze text{
 color: #fff;
 text-align:right;
}
.freeze text:nth-child(1){
  font-size: 22rpx;
}
.freeze text:nth-child(2){
  font-size: 24rpx;
}
.rental view {
  position: absolute;
  left:30rpx;
  display: flex;
  align-items:flex-end;
}
.rental view text:nth-child(1) {
  font-size: 36rpx;
}
.rental view text:nth-child(2) {
  font-size: 60rpx;
  position:relative;
  bottom:-4rpx;
}
.nav{
  display: flex;
  margin-top: 40rpx;
}
.nav view{
  margin:auto;
  width:248rpx;
  height:248rpx;
}
.nav view navigator {
  width:100%;
  height:100%;
  display:flex;
  flex-direction:column;
  align-items:center;
  justify-content:center;
}
.nav view navigator image{
  width:80rpx;
  height:80rpx;
}
.nav view navigator text{
  font-size:34rpx;
  padding:16rpx 0;
  color:#999;
}
.extract{
  position:absolute;
  bottom:30rpx;
  right:30rpx;
  color:#fff;
  font-size:32rpx;
}

